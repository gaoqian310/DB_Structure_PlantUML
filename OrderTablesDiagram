@startuml
object orders {
    order_id 
    wave_id 
    user_id 
    discount_id 
    shipping_type 
    status 
    order_date
    order_refund_status
    }


object line_items{
    line_items_id 
    order_id 
    product_id 
    variant_id 
    discount_id
    line_items_quantity
    }


object refunds{
    refunds_id 
    line_items_id
    refunds_status 
    refunds_date
    }


object transactions{
    transactions_id 
    order_id 
    refunds_id
    amount
    transaction_time
    }


object carts{
    cart_id 
    user_id 
    product_id
    varient_id 
    quantity
    discount_id 
    }


object delivery_method{
    delivery_method_id
    delivery_method
    }

object refunds_transactions{
    refunds_transactions_id 
    refunds_id
    refunds_amount
    refunds_transaction_time
    }

orders ---{ line_items
line_items }- refunds
orders -- transactions
carts - orders
orders - delivery_method
refunds - refunds_transactions
@enduml
